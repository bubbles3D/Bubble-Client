FirstPerson/lens
{
  zorder 200
	container Panel(lensContener)
	{
		//material     viseur1
		
		metrics_mode relative
		width 0.1
		height 0.1
		top 0.45
		left 0.45

		material     viseur1

	}
}

FirstPerson/life
{
  zorder 200
	container Panel(life)
	{
		material     life
		width 0.1
		height 0.1
		horz_align left
		vert_align top
		left 0.85
		top 0.04
	}

}
